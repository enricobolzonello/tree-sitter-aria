{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "aria",
      "camelcase": "Aria",
      "title": "Aria",
      "scope": "source.aria",
      "file-types": ["aria"],
      "injection-regex": "^aria$",
      "class-name": "TreeSitterAria",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree Sitter parser for the Aria lang",
    "authors": [
      {
        "name": "Borja Castellano",
        "email": "borjacastellano1@gmail.com",
        "url": "https://bcastellano.com/portfolio/"
      }
    ],
    "links": {
      "repository": "https://www.github.com/ZocoLini/tree-sitter-aria"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
